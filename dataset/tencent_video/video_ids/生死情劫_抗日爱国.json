            "year": "2003",
            "imgtag_ver": "{\"tag_1\":{\"id\":\"-1\",\"param\":\"\",\"param2\":\"\",\"text\":\"\"},\"tag_2\":{\"id\":\"-1\",\"param\":\"\",\"param2\":\"\",\"text\":\"\"},\"tag_3\":{\"id\":\"30001\",\"param\":\"1X=;2X=\",\"param2\":\"1X=;2X=\",\"text\":\"全22集\"},\"tag_4\":{\"id\":\"-1\",\"param\":\"\",\"param2\":\"\",\"text\":\"\"}}",
            "type": "2",
