icipants":[{"characterDetails":{"character":"Host","characterId":"3507","language":null},"displayName":"Keegan-Michael Key","order":1,"participantId":"1332","role":"Actor","sortName":"Key, Keegan-Michael"}],"programId":"cea5bbd6-4141-470c-9d38-88ab3a3d0684","programType":"episode","ratings":[{"advisories":[],"description":null,"filingNumber":null,"system":"TVPG","value":"TV-PG"}],"releases":[{"releaseDate":"2020-02-17","releaseOrg":null,"releaseType":"original","releaseYear":2020,"territory":null}],"seasonId":"8fb333ef-8ac3-4ba5-9130-9ffd3439901e","seasonSequenceNumber":8,"seriesId":"3f6d6514-f6a0-4f4b-b6cc-23a186014f0f","seriesType":"standard","sport":null,"tags":[{"displayName":null,"type":"titleEidr","value":"F62C-D05E-A982-3E21-73FA-G"},{"displayName":null,"type":"disneyPlusOriginal","value":"false"}],"targetLanguage":"en","texts":[{"field":"description","type":"full","language":"en","content":"National Geographic has reimagined BRAIN GAMES and added a Hollywood twist to the classic mind-bending format. Hosted by Keegan-Michael Key, this new season of BRAIN GAMES challenges some of the world’s biggest celebrities to realize their special brain power through fun and highly entertaining interactive games, illusions and social experiments that will reveal the “why” behind the “wow.”","sourceEntity":"season","targetEntity":"season"},{"field":"description","type":"brief","language":"en","content":"Keegan-Michael Key welcomes billionaire CEO and star of “Shark Tank” Mark Cuban to the show.","sourceEntity":"program","targetEntity":"program"},{"field":"description","type":"full","language":"en","content":"Are you ready for a truly mind-blowing experience? The Emmy® Award nominated Brain Games features interactive games and experiments designed to mess with your mind and reveal the inner-workings of your brain, exploring the science of perception, memory, attention, illusion, stress, morality, attraction, anger, and much more. Hailed by critics as \"tremendous fun\" that \"makes science entertaining,\" Brain Games turns your mind's eye inwards for a fascinating journey into the three and a half pounds of tissue that makes you... you.","sourceEntity":"series","targetEntity":"series"},{"field":"description","type":"full","language":"en","content":"Keegan-Michael Key welcomes billionaire CEO and star of “Shark Tank” Mark Cuban to explore how business-savvy, powerful brains might be different from others, and how power can manipulate the way we think. Cuban and Key undergo a series of challenges involving split-second decision-making via rapid hiring and firing and memory mastery. Plus, actor Colin Hanks makes a special guest appearance.","sourceEntity":"program","targetEntity":"program"},{"field":"description","type":"medium","language":"en","content":"Keegan-Michael Key welcomes billionaire CEO and star of “Shark Tank” Mark Cuban to explore how business-savvy, powerful brains might be different from others, and how power can manipulate the way we think. Cuban and Key undergo a series of challenges involving split-second decision-making via rapid hiring and firing and memory mastery. Plus, actor Colin Hanks makes a special guest appearance.","sourceEntity":"program","targetEntity":"program"},{"field":"title","type":"full","language":"en","content":"Brain Games","sourceEntity":"series","targetEntity":"series"},{"field":"title","type":"sort","language":"en","content":"mark cuban: power and persuasion","sourceEntity":"program","targetEntity":"program"},{"field":"title","type":"slug","language":"en","content":"brain-games","sourceEntity":"series","targetEntity":"series"},{"field":"title","type":"sort","language":"en","content":"brain games","sourceEntity":"series","targetEntity":"series"},{"field":"description","type":"medium","language":"en","content":"Are you ready for a truly mind-blowing experience? The Emmy® Award nominated Brain Games features interactive games and experiments designed to mess with your mind and reveal the inner-workings of your brain, exploring the science of perception, memory, attention, illusion, stress, morality, and much more.","sourceEntity":"series","targetEntity":"series"},{"field":"description","type":"brief","language":"en","content":"This new season of BRAIN GAMES challenges some of the world’s biggest celebrities.","sourceEntity":"season","targetEntity":"season"},{"field":"title","type":"full","language":"en","content":"Mark Cuban: Power and Persuasion","sourceEntity":"program","targetEntity":"program"},{"field":"description","type":"medium","language":"en","content":"National Geographic has reimagined BRAIN GAMES and added a Hollywood twist to the classic mind-bending format. Hosted by Keegan-Michael Key, this new season of BRAIN GAMES challenges some of the world’s biggest celebrities to realize their special brain power through fun and highly entertaining interactive games, illusions and social experiments that will reveal the “why” behind the “wow.”","sourceEntity":"season","targetEntity":"season"},{"field":"title","type":"slug","language":"en","content":"mark-cuban-power-and-persuasion","sourceEntity":"program","targetEntity":"program"},{"field":"title","type":"full","language":"en","content":"Brain Games","sourceEntity":"season","targetEntity":"season"},{"field":"description","type":"brief","language":"en","content":"Brain Games features games & experiments revealing the science of how your brain works.","sourceEntity":"series","targetEntity":"series"},{"field":"title","type":"sort","language":"en","content":"brain games","sourceEntity":"season","targetEntity":"season"},{"field":"title","type":"slug","language":"en","content":"brain-games","sourceEntity":"season","targetEntity":"season"}],"type":"DmcVideo","typedGenres":[],"videoArt":[],"videoId":"b920d794-451a-4706-bc68-9422abf2a377"}}}}